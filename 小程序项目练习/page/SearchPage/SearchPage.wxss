@import "../FreeGetVolume/Item/item.wxss";
.container{
  background-color: #eee;
  width: 100vw;
  height: 100vh;
}
.searchView{
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.searchContainerView{
  height: 80rpx;
  width: 80vw;
  border: 1px solid gray;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
  margin-top: 10rpx;
}
.searchTextStyle{
  font-size: 30rpx;
  padding-left: 10rpx;
}
.searchImageStyle{
  height: 60rpx;
  width: 60rpx;
  padding-left: 10rpx;
}
.shopListContainer{
  display: flex;
  flex-direction: column;
}
.shopListItemStyle{
  height: 300rpx;
  background-color: salmon;
  border: 1px solid black;
}